{
  "name": "midievents",
  "version": "2.0.0",
  "description": "Decode/encode MIDI events.",
  "main": "src/midievents",
  "author": "Nicolas Froidure",
  "repository": {
    "type": "git",
    "url": "http://github.com/nfroidure/midievents.git"
  },
  "metapak": {
    "data": {
      "files": "src/*.js",
      "testsFiles": "src/*.mocha.js"
    },
    "configs": [
      "readme",
      "mocha",
      "eslint",
      "codeclimate",
      "travis"
    ]
  },
  "license": "MIT",
  "engines": {
    "node": ">=6.9.5"
  },
  "devDependencies": {
    "commitizen": "^4.3.0",
    "conventional-changelog-cli": "^1.3.8",
    "coveralls": "^3.0.0",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^4.19.0",
    "eslint-plugin-prettier": "^2.6.0",
    "istanbul": "^0.4.5",
    "metapak": "1.0.3",
    "metapak-nfroidure": "6.0.0",
    "mocha": "^10.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "prettier": "^1.11.1"
  },
  "scripts": {
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "cli": "env NODE_ENV=${NODE_ENV:-cli}",
    "cover": "istanbul cover _mocha --report html -- src/*.mocha.js -R spec -t 5000",
    "coveralls": "istanbul cover _mocha --report lcovonly -- src/*.mocha.js -R spec -t 5000 && cat ./coverage/lcov.info | coveralls && rm -rf ./coverage",
    "cz": "env NODE_ENV=${NODE_ENV:-cli} git cz",
    "lint": "eslint src/*.js",
    "metapak": "metapak",
    "mocha": "mocha src/*.mocha.js",
    "prettier": "prettier --write src/*.js",
    "preversion": "npm t && npm run lint && npm run metapak -s",
    "test": "npm run mocha",
    "version": "npm run changelog && git add CHANGELOG.md"
  },
  "keywords": [
    "sound",
    "music",
    "midi",
    "event",
    "events",
    "encode",
    "decode"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "greenkeeper": {
    "ignore": [
      "commitizen",
      "cz-conventional-changelog",
      "conventional-changelog-cli",
      "mocha",
      "mocha-lcov-reporter",
      "coveralls",
      "istanbul",
      "eslint",
      "eslint-config-prettier",
      "prettier"
    ]
  }
}